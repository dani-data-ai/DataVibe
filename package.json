{
  "name": "datavibe",
  "version": "1.0.0",
  "description": "Lovable AI for Databases - Cloud-only natural language database interaction",
  "private": true,
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && python -m uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && pip install -r requirements.txt",
    "start:backend": "cd backend && python -m uvicorn main:app --host 0.0.0.0 --port 8000",
    "test": "npm run test:backend",
    "test:backend": "cd backend && python -m pytest"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "database",
    "ai",
    "natural-language",
    "sql",
    "llm",
    "cloud-only",
    "codespaces"
  ],
  "author": "DataVibe Team",
  "license": "MIT"
}